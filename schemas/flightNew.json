{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://ranner.com/flightNew.schema.json",
  "type": "object",
  "properties": {
    "tripId": {
      "type": "number",
      "minLength": 1,
      "maxLength": 50
    },
    "flightOfferId": {
      "type": "string",
      "minLength": 1,
      "maxLength": 50
    },
    "outboundFlightNumber": {
      "type": "string",
      "minLength": 1,
      "maxLength": 20
    },
    "inboundFlightNumber": {
      "type": "string",
      "minLength": 1,
      "maxLength": 20
    }
  },
  "additionalProperties": false,
  "required": [
    "tripId",
    "outboundFlightNumber",
    "inboundFlightNumber"
  ]
}
