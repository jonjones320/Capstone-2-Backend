{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/example.json",
  "type": "object",
  "properties": {
    "username": {
      "type": "string",
      "minLength": 1,
      "maxLength": 33
    },
    "email": {
      "type": "string",
      "minLength": 6,
      "maxLength": 67,
      "format": "email"
    },
    "password": {
      "type": "string",
      "minLength": 7,
      "maxLength": 31
    },
    "firstName": {
      "type": "string",
      "minLength": 1,
      "maxLength": 39
    },
    "lastName": {
      "type": "string",
      "minLength": 1,
      "maxLength": 39
    },
    "isAdmin": {
      "type": "boolean"
    }
  },
  "additionalProperties": false,
  "required": [
    "username",
    "email",
    "password"
  ]
}
